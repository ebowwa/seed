# Copy this file to .env and fill in your values
# SAVE ALL THESE TO DOPPLER
# GitHub Personal Access Token (for gh cli)
# Generate at: https://github.com/settings/tokens
GITHUB_TOKEN=

# Anthropic API Key (for Claude Code)
# Note: If you have Claude Pro/Team, you can leave this empty and use browser auth!
# Get from: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=

# Tailscale Auth Key (optional, for headless setup)
# Generate at: https://login.tailscale.com/admin/settings/keys
TAILSCALE_AUTH_KEY=

# Z.AI API Key (for Vision MCP Server and GLM models)
# Get from: https://z.ai/manage-apikey/apikey-list
Z_AI_API_KEY=

# Z.AI Configuration (for Claude Code with Z.ai backend)
# Leave these empty to use default Anthropic endpoint
ANTHROPIC_BASE_URL=https://api.z.ai/api/anthropic
ANTHROPIC_MODEL=glm-4.7
ANTHROPIC_SMALL_FAST_MODEL=glm-4.6

# Doppler Configuration (for secret management)
DOPPLER_PROJECT=seed
DOPPLER_CONFIG=prd

# Doppler Service Token (optional, for automated setup)
# Get from: https://dashboard.doppler.com/workplace/[your-workplace]/projects
DOPPLER_TOKEN=

# Supabase MCP Server Configuration (optional, for database integration)
# Get from: https://supabase.com/dashboard/account/tokens
SUPABASE_PAT=

# Your Supabase project URL (extract project_ref from URL)
# Example: https://[PROJECT_REF].supabase.co
SUPABASE_URL=

# Your Supabase project reference (from URL above)
SUPABASE_PROJECT_REF=

# GitHub MCP Server Configuration (optional, for advanced GitHub operations)
# Get from: https://github.com/settings/tokens
GITHUB_PAT=

# Your GitHub username (for GitHub MCP server)
GITHUB_USERNAME=

# Your GitHub organization (optional, for organization-level operations)
GITHUB_ORG=

# Optional: Custom installation paths
# INSTALL_PATH=/usr/local/bin

# Optional: Skip specific installations (set to 'true' to skip)
# SKIP_CLAUDE=false
# SKIP_GITHUB=false
# SKIP_TAILSCALE=false
# SKIP_DOPPLER=false